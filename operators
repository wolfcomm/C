#include <stdio.h>
int main ()
{
    int a = 9;
    printf("a %d\n", a);
    
    int c = a++;
    printf("c %d and a %d\n", c, a);
    
    int d = --a;
    printf("d %d and a %d\n", d, a);
    
    int b = a % 4;
    printf("b %d and a %d\n", b, a);
    
    float fa = 5.0 / .5;
    printf("fa %f\n", fa);
    
    fa *= 10;
    printf("fa %f\n", fa);
    
    a = fa++;
    printf("%fa\n", fa);
    
    
    
    
        return 0;
}
