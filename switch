#include <stdio.h>

int main()
{
char a = 'a';
    char b = 'b';
    char c = 'c';
    char letter = 'b';
    switch (letter) {
        case 'a':
            printf("letter %c\n", a);
            break;
        case 'b':
            printf("letter %c\n", b);
            break;
        case 'c':
            printf("letter %c\n", c);
            break;
        default:
            printf("letter %c not found\n", letter);
            break;
    }
